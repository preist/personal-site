{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "style:lint": "stylelint '**/*.{css,scss,sass}' --ignore-path .gitignore --allow-empty-input",
    "style:fix": "stylelint '**/*.{css,scss,sass}' --fix --ignore-path .gitignore --allow-empty-input",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run style:lint"
  },
  "dependencies": {
    "@strapi/blocks-react-renderer": "^1.0.2",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.93.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "stylelint": "^16.24.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5"
  }
}
